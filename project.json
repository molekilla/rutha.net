{
  "version": "0.3.0",
  "webroot": "dist",
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "code": [
    "**/*.cs"
  ],
  "dependencies": {
    "Kestrel": "1.0.0-*",
    "Microsoft.AspNet.Hosting": "1.0.0-*",
    "Microsoft.AspNet.Diagnostics": "1.0.0-*",
    "Microsoft.AspNet.Security": "1.0.0-*",
    "Microsoft.AspNet.Identity": "1.0.0-*",
    "Microsoft.AspNet.Mvc": "6.0.0-beta2",
    "Microsoft.AspNet.Mvc.Razor": "6.0.0-beta2",
    "mongocsharpdriver": "2.0.0-*",
    "MongoDB.AspNet.Identity": "1.0.5",
    "Microsoft.AspNet.Security.Cookies": "1.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-*",
    "Microsoft.Framework.Logging": "1.0.0-*",
    "Microsoft.Framework.Logging.Console": "1.0.0-*",
    "Microsoft.Framework.ConfigurationModel": "1.0.0-*",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-*",
    "Microsoft.Framework.ConfigurationModel.Xml": "1.0.0-*",
    "Microsoft.CodeAnalysis.CSharp": "1.0.0.0-*",
      "Microsoft.Framework.DependencyInjection": "1.0.0-beta2"
  },
  "commands": {
    "test": "xunit.runner.kre",
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:3002",
    "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:3002"
  },
  "exclude": [
    "node_modules"
  ],
  "frameworks": {
    "aspnet50": {},
    "aspnetcore50": {}
  }
}